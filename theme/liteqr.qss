QWidget{
    font-family: "Abel";
    font-size: 12pt;
    color: "gray";
}
QMenuBar{
    color: "black";
}
QMenuBar::selected{
    color: "gray";
}
QMenu{
    color: "black";
}
QMenu::selected{
    color: "gray";
}
QMessageBox{
    background-color: "black";
    color: "white";
}
QPushButton{
    padding: 2px;
    border-radius: 3px;
    border-width: 1px;
    border-style: solid;
    border-color: "white";
    color: "white";
}
QPushButton::hover{
    background-color: "white";
    color: "gray";
}
QTextEdit{
    padding: 5px;
    border-radius: 3px;
    border-width: 1px;
    border-style: solid;
    border-color: "white";
    color: "gray";
}
